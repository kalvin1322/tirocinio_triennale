{
  "UNet_V1": {
    "description": "U-Net architecture with customizable encoder-decoder pairs",
    "default_params": {
      "in_channels": 1,
      "out_channels": 1,
      "num_encoders": 3,
      "start_middle_channels": 64
    },
    "tunable_params": {
      "num_encoders": {
        "type": "int",
        "min": 2,
        "max": 5,
        "default": 3,
        "description": "Number of encoder-decoder pairs"
      },
      "start_middle_channels": {
        "type": "int",
        "options": [32, 64, 128, 256],
        "default": 64,
        "description": "Starting number of middle channels (doubles each level)"
      }
    }
  },
  "ThreeL_SSNet": {
    "description": "Three-Level Squeeze-and-Excitation Network",
    "default_params": {
      "in_channels": 1,
      "out_channels": 1
    },
    "tunable_params": {}
  },
  "SimpleResNet":{
    "description": "A simple residual network (FCN) for post-processing",
    "default_params": {
      "in_channels": 1,
      "out_channels": 1
    },
    "tunable_params": {
      "num_layers": {
        "type": "int",
        "min": 2,
        "max": 10,
        "default": 4,
        "description": "Number of residual layers in the network"
      },
      "features": {
        "type": "int",
        "options": [16, 32, 64, 128],
        "default": 32,
        "description": "Number of features in each convolutional layer"
      }
    }
  
  },
  "PostProcessNet": {
    "description": "A generic post-processing network template",
    "default_params": {
      "in_channels": 1,
      "out_channels": 1
    },
    "tunable_params": {
      "hidden_channels": {
        "type": "int",
        "options": [16, 32, 64, 128],
        "default": 32,
        "description": "Number of hidden channels in the network"
      },
      "use_residual": {
        "type": "bool",
        "default": true,
        "description": "Whether to use residual connections"
      }
    }
  }

}
